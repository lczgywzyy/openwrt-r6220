/dts-v1/;

/include/ "mt7621s.dtsi"

/ {
	compatible = "mediatek,mt7621-eval-board", "mediatek,mt7621-soc";
	model = "Netgear R6220";

	cpus {
		cpu@0 {
			compatible = "mips,mips1004Kc";
		};
	};

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x8000000>;
	};

	chosen {
		bootargs = "console=ttyS0,57600";
	};

	pinctrl {
		state_default: pinctrl0 {
			gpio {
				ralink,group = "i2c";
				ralink,function = "gpio";
			};
		};
	};

	nand@1e003000 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "mtk,mt7621-nand";

		partition@0 {
			label = "uboot";
			reg = <0x0 0x100000>;
			read-only;
		};

		partition@200000 {
			label = "rootfs";
			reg = <0x200000 0x2000000>;
			read-only;
		};

		factory: partition@2d00000 {
			label = "factory";
			reg = <0x2d00000 0x100000>;
			read-only;
		};

	};

	pcie@1e140000 {
		status = "okay";

		pcie0 {
			mt76@0,0 {
				reg = <0x0000 0 0 0 0>;
				device_type = "pci";
				mediatek,mtd-eeprom = <&factory 0x0004>;
				mediatek,2ghz = <0>;
			};
		};

		pcie1 {
			mt76@1,0 {
				reg = <0x0000 0 0 0 0>;
				device_type = "pci";
				mediatek,mtd-eeprom = <&factory 0x8004>;
				mediatek,5ghz = <0>;
			};
		};
	};

	gpio-leds {
		compatible = "gpio-leds";

		wifi {
			label = "r6220:green:wifi";
			gpios = <&gpio0 0 1>;
		};

		power {
			label = "r6220:green:power";
			gpios = <&gpio0 9 1>;
		};
	};

	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		wps {
			label = "wps";
			gpios = <&gpio0 10 1>;
			linux,code = <0x211>;
		};

		reset {
			label = "reset";
			gpios = <&gpio0 11 0>;
			linux,code = <0x198>;
		};
	};
};
